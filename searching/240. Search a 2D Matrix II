class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
       for (int[] row : matrix){
        if(binarySearch(row,target)){
            return true;
        }
        
        
       }
       return false;


    }
    private boolean binarySearch(int[] row, int target) {
        int start = 0, end = row.length - 1;
        while (start <= end) {
            int mid = start + (end - start) / 2;
            if (row[mid] == target){
             return true;
            }
            else if (row[mid] < target) {
                start = mid + 1;
            }
            else 
            {
                end = mid - 1;
            }
        }
        return false;
    }
}
    
